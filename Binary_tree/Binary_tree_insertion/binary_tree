
class Node:
    def __init__(self,data):
        self.data=data
        self.left=None
        self.right=None

class binary_tree:
    def __init__(self,data):
        self.root=Node(data)

tree=binary_tree(1)
tree.root.left=Node(2)
tree.root.right=Node(3)
tree.root.left.left=Node(4)
tree.root.left.right=Node(5)
